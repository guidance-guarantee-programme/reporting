<div class="row">
  <div class="col-md-12">
    <h1>Edit appointment summary</h1>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= form_for @appointment_summary do |f| %>
      <%= render 'form', f: f %>

      <div class="form-group">
        <%= link_to 'Cancel', appointment_summaries_path, class: 'btn btn-default' %>
        <%= f.submit 'Update', class: 'btn btn-success t-update-button' %>
      </div>
    <% end %>
  </div>
</div>
