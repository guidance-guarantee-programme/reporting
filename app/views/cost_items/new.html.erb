<div class="row">
  <div class="col-md-12">
    <h1>Cost Item - <%= @cost_item.name %></h1>
  </div>
</div>

<%= form_for @cost_item do |f| %>
  <%= render 'form', f: f %>

  <div class="form-group">
    <%= link_to 'Cancel', cost_items_path, class: 'btn btn-default' %>
    <%= f.submit 'Create', class: 'btn btn-success t-update-button' %>
  </div>
<% end %>
